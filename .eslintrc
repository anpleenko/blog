{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "max-len": ["error", 800]
    },
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    }
}
